        "1647522524cac8af89-1": {
          "type": "slide",
          "settings": {
            "heading": "",
            "heading_size": "h1",
            "subheading": "I made a purchase before Christmas and today. Very helpful, no pressure to make a purchase - but it would be difficult to resist. <br> <span>- Ann Wildcat <span><\/span><\/span>",
            "button_label": "",
            "link": "",
            "button_style_secondary": false,
            "box_align": "middle-center",
            "show_text_box": true,
            "text_alignment": "center",
            "image_overlay_opacity": 0,
